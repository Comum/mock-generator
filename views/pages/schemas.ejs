<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width">
        <title>SMX - Mock Generator</title>
    </head>
    <body>
        <h1>Schemas information</h1>
        <% structure.folders.forEach(function (folder) { %>
            <div><%= folder.folder %></div>
            <ul>
                <% folder.files.forEach(function (file) { %>
                    <li>
                        <a href="/schemas-json/<%= folder.folder %>/<%= file %>"><%= file %></a>
                    </li>
                <% }) %>
            </ul>
        <% }) %>
    </body>
</html>